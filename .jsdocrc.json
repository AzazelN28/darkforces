{
  "plugins": ["plugins/markdown"],
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "template": "node_modules/minami",
    "encoding": "utf8",
    "destination": "dist/docs",
    "recurse": true,
    "tutorials": "docs/tutorials",
    "package": "./package.json",
    "readme": "./README.md"
  }
}
